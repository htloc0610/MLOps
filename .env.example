# Configuration for Vertex AI Pipeline
# Copy this file to .env to use your actual values

# ============================================================================
# GOOGLE CLOUD PROJECT CONFIGURATION
# ============================================================================

# Your GCP Project ID
PROJECT_ID=project-1cd612d2-3ea2-4818-a72

# GCP Region where resources are deployed
REGION=us-central1

# ============================================================================
# GOOGLE CLOUD STORAGE
# ============================================================================

# Name of your GCS bucket (without gs:// prefix)
BUCKET_NAME=housing-data-project-1cd612d2-3ea2-4818-a72

# Folder within bucket for pipeline artifacts
PIPELINE_ROOT_FOLDER=pipeline_root_houseprice

# Path to dataset within bucket
DATA_PATH=data/Housing.csv

# ============================================================================
# ARTIFACT REGISTRY (Docker Image Repository)
# ============================================================================

# Artifact Registry repository name
REPOSITORY=mlops-repo

# Docker image name
IMAGE_NAME=training-image

# Docker image tag
IMAGE_TAG=latest

# ============================================================================
# HYPERPARAMETERS (Optional - can be modified)
# ============================================================================

# Random Forest hyperparameters
N_ESTIMATORS=100
MAX_DEPTH=10
RANDOM_STATE=42
